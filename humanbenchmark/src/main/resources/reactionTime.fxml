<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<StackPane fx:id = "root"
            xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="main.backend.ReactionTime"
            style="-fx-background-color: blue; -fx-background-radius: 100">

        <VBox spacing="10" alignment="CENTER">
                <Button fx:id="interact"
                        textAlignment="CENTER"
                        text="Reaction Time Test"
                        style="-fx-font-size: 25; -fx-background-color: transparent"
                        onAction="#interact"/>
        </VBox>
</StackPane>
